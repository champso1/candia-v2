set terminal pngcairo enhanced color notransparent

set border lw 2
set size ratio 0.65

set output "up.png"
set label "Candia-v2 up-quark evolution from" at 0.12,0.7
set label "Les Houches initial conditions" at 0.12,0.68
set ylabel "xf_u(x,Q^2)" offset 2,0
set xlabel "x" offset 0,0.7

# set logscale x
set xrange [0.01:0.3]
set yrange [0.4:0.8]

set format x "%.2f"

set xtics ("10@^{-2}" 1e-2, "0.05" 0.05, "0.1" 0.1, "0.15" 0.15, "0.2" 0.2, "0.25" 0.25, "0.3" 0.3)

plot 'lo.dat' using 1:3 with lines lw 2 dt 4 title "LO",\
'nlo.dat' using 1:3 with lines lw 2 dt 2 title "NLO",\
'nnlo.dat' using 1:3 with lines lw 2 dt 1 lc rgb "blue" title "NNLO"

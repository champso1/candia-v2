set terminal pdfcairo enhanced color notransparent

set output "ns2.pdf"
set ylabel "xP_{ns}^{(2)}(x)"
set xlabel "x"
set size ratio 1

set logscale x
set format x "%.0e"
set yrange [-1.0:5.0]
set xrange [1.0e-4:1.0]

plot 'ns.dat' using 1:2 with lines lw 2 title '-',\
'' using 1:3 with lines lw 2 title '+'

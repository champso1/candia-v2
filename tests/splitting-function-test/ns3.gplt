set terminal pdfcairo enhanced color notransparent

set output "ns3.pdf"
set ylabel "xP_{ns}^{(3)}(x)"
set xlabel "x"

set size ratio 1
# set logscale x
set format x "%.1f"
set yrange [-0.6:0.6]
set xrange [1.0e-4:1.0]
#set xtics (0.0 '0', 0.2 '0.2', 0.4 '0.4', 0.6 '0.6', 0.8 '0.8', 1.0 '1')

plot 'ns.dat' using 1:6 with lines lw 2 dt 2 title "- approx1",\
'' using 1:7 with lines lw 2 dt 2 title "- approx2",\
'' using 1:8 with lines lw 2 dt 4 title "+ approx1",\
'' using 1:9 with lines lw 2 dt 4 title "+ approx2"

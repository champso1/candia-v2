set terminal pdfcairo enhanced color notransparent
set output "grid.pdf"

set xrange [1.0e-7:1.0]
set logscale x
set format x "%.0e"
set key center top

xmin=1.0e-7
xmax=1.0
bins=50.0
binwidth=(xmax-xmin)/bins
bin(x,w)=w*floor(x/w)

plot 'points.dat' using (bin($1,binwidth)):(1.0) smooth freq with boxes
